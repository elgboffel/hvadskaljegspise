@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using HvadSkalJegSpise.Web.Models.ModelsBuilder

@{
    Layout = null;
    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.4.1/jquery-migrate.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("https://ajax.aspnetcdn.com/ajax/jquery.validate/1.13.1/jquery.validate.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("https://ajax.aspnetcdn.com/ajax/mvc/5.1/jquery.validate.unobtrusive.min.js", 1, 1, new { @defer = string.Empty })
        .RequiresJs("~/Content/components/picturefill/src/picturefill.js", 1, 1, new { @defer = string.Empty })
        .RequiresCss("~/content/css/bootstrap.css")
            .RequiresCss("http://fonts.googleapis.com/css?family=Ubuntu:300,400,500,700")
            .RequiresCss("http://fonts.googleapis.com/css?family=Lekton:400,700,400italic")
            .RequiresCss("~/content/css/main-oak.css")
            .RequiresCss("~/content/css/bootstrap.css")
            .RequiresCss("~/content/css/bootstrap.css")

        .RequiresCss("~/content/css/bootstrap.css")
        .RequiresCss("~/content/css/main.css");

    }
