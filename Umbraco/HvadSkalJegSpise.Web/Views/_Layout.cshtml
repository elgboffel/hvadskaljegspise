<html lang="@Model.CurrentCulture.TwoLetterISOLanguageName" prefix="og: http://ogp.me/ns#">
<head>
    <!-- TODO: Husk at fjern til live-->
    <meta NAME="robots" content="noindex, nofollow">
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*@Html.Raw(SEOChecker.Library.MetaData.Current.RenderTags())*@
    @*@Html.CachedPartial("_Favicons", Model, 3600, false)*@
    @Html.CachedPartial("_OpenGraph", Model, 300, true)
    <link rel="alternate" type="application/rss+xml" title="@CurrentPage._siteName" href="rss.xml" />
    @Html.RenderCssHere()
</head>
<body ng-app="ng-app">
    @Html.CachedPartial("_Header", Model, 3600, cacheByPage: true)
    @*@Html.CachedPartial("_MobileNav", Model, 3600, cacheByPage: true)*@
    <div class="body-content">
        @RenderBody()
    </div>
    @Html.CachedPartial("_Footer", Model, 3600)
    @Html.Partial("_CookieConsent")
    <!-- ANALYTICS -->
    @*@Html.Raw(website.Analytics)*@

    @Html.RenderJsHere()
</body>
</html>
